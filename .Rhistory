# Plot.Freq by Month :split by pandemic phrase
plot_Month <- ggplot(data=Robbery, aes(Month)) +
geom_histogram(col="darkblue",
fill="lightblue",
alpha = .7) +
labs(x="Month", y="Frequency") +
labs(title="B1.Monthly Robbery frequency") + scale_x_continuous(breaks=1:12) + geom_text(stat='count', aes(label=..count..),vjust = 1.5)
